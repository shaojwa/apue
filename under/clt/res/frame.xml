<?xml version="1.0" encoding="utf-8"?>
<Window mininfo="360,200" mininfo="540,360" size="720,480" maxinfo="1080,720">
    <Font name="Tahoma" id="1" size="12" default="true" />
    <VerticalLayout bkcolor="#FFFFFFFF">
        <HorizontalLayout name="hlOptions" height="25" inset="5,5,5,5" childpadding="5" bkcolor="#FFD7DEF0" >
